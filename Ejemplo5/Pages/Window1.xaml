<Window x:Name="back" x:Class="Ejemplo5.Pages.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ejemplo5.Pages"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <Grid.Resources>

            <Style TargetType="Button" BasedOn="{StaticResource buttonStyle1}"/>
        </Grid.Resources>
        <Grid.Background>
            <ImageBrush ImageSource="/Ejemplo5;component/imagen/fondo.jpg"/>
        </Grid.Background>
        <Grid Height="200" Width="600" Background="White">
            
            <Label Content="QUIENES SOMOS" HorizontalAlignment="Center" FontSize="24" FontFamily="Britannic Bold" FontWeight="Bold" FontStyle="Italic" Foreground="#FFF4D303"/>
            <Label x:Name="label" Margin="30,50,30,30" FontSize="18"/>



        </Grid>
        <Button Margin="0,350,0,0" Content="VOLVER" Click="Button_Click"/>

    </Grid>
</Window>
